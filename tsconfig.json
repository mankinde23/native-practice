{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
     "src/**/*.ts", 
    "typings/**/*.d.ts", "app/index.jsx", "app/profile.jsx", "app/_layout.jsx", "app/+html.jsx", "app/+not-found.jsx",
  ]
}
